msgstr "256 bitars AES, 2048 bytes per kommando -> 880 bytes per sekund"

#: ../storage/faq.rst:56
msgid "128 bit AES, 2048 bytes per command -> 893 bytes per second"
msgstr "128 bitars AES, 2048 bytes per kommando -> 893 bytes per sekund"

#: ../storage/faq.rst:57
msgid "256 bit AES, 240 bytes per command -> 910 bytes per second"
msgstr "256 bitars AES, 240 bytes per kommando -> 910 bytes per sekund"

#: ../storage/faq.rst:58
msgid "128 bit AES, 240 bytes per command -> 930 bytes per second"
msgstr "128 bitars AES, 240 bytes per kommando -> 930 bytes per sekund"

