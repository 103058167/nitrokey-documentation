msgstr "256-bittinen AES, 2048 tavua per komento -> 880 tavua sekunnissa"

#: ../storage/faq.rst:56
msgid "128 bit AES, 2048 bytes per command -> 893 bytes per second"
msgstr "128-bittinen AES, 2048 tavua per komento -> 893 tavua sekunnissa"

#: ../storage/faq.rst:57
msgid "256 bit AES, 240 bytes per command -> 910 bytes per second"
msgstr "256-bittinen AES, 240 tavua per komento -> 910 tavua sekunnissa"

#: ../storage/faq.rst:58
msgid "128 bit AES, 240 bytes per command -> 930 bytes per second"
msgstr "128-bittinen AES, 240 tavua per komento -> 930 tavua sekunnissa"

